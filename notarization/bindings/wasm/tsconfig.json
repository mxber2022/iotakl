{
  "compilerOptions": {
    "lib": [
      "ES2020",
      "DOM"
    ],
    "target": "ES2022",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}
